<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="app.login.LoginController">
        <children>
            <Label  text="Login">
                <font>
                    <Font size="40.0" />
                </font>
            </Label>
            <Label text="personnummer:"  />
            <TextField fx:id="socialNoInput"  />
            <Label  text="lösenord:"  />
            <TextField fx:id="passwordInput"  />
            <Button fx:id="inloggBtn" onAction="#login" text="Logga in" />
            <Label  fx:id="errorNoInput" style="visibility: hidden" text="Vänligen fyll i båda fälten" />
            <Label  fx:id="errorWrongInput" style="visibility: hidden" text="Fel användaruppgifter" />

        </children>
</VBox>
