<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="50.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.transaction.TransactionController">
   <children>
      <HBox prefWidth="495.0" spacing="60.0">
         <children>
         <VBox>
            <children>
               <Label fx:id="message" prefHeight="29.0" prefWidth="132.0" text="Message">
                  <font>
                     <Font size="14.0" />
                  </font></Label>
               <Label fx:id="date" prefHeight="15.0" prefWidth="204.0" text="Date">
                  <font>
                     <Font size="12.0" />
                  </font></Label>
            </children>
         </VBox>

            <Label fx:id="amount" text="Amount">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin></Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
